{
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 4,
    "useTabs": false,
    "printWidth": 80,
    "htmlWhitespaceSensitivity": "ignore",
    "jsxSingleQuote": true,
    "endOfLine": "auto",
    "bracketSpacing": true,
    "arrowParens": "always",
    "overrides": [
        {
            "files": "*.js",
            "options": {
                "htmlWhitespaceSensitivity": "ignore"
            }
        }
    ]
}
